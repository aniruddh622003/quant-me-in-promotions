    <mj-raw>
      <meta name="color-scheme" content="light dark">
      <meta name="supported-color-schemes" content="light dark">
    </mj-raw>
    <mj-attributes>
      <mj-all font-family="'Helvetica Neue', Helvetica, Arial, sans-serif"></mj-all>
      <mj-text font-size="16px" color="#ffffff" line-height="24px"></mj-text>
      <mj-button background-color="#ffffff" color="#4800ac" font-weight="bold" border-radius="25px" inner-padding="15px 30px"></mj-button>
      <mj-section padding="0px"></mj-section>
    </mj-attributes>
    <mj-style>
      .bold-text {
        font-weight: 700;
        color: #00e676; /* A slight green accent for "Go" signals/money */
      }
      .feature-icon {
        width: 50px;
        margin-bottom: 10px;
      }
      /* GMAIL HACK: Use linear-gradient to force background color to stick in Dark Mode */
      .force-bg {
        background-color: #370085 !important;
        background-image: linear-gradient(#370085, #6200ea) !important;
      }
      .force-purple-text {
        color: #8300caff !important;
      }
      /* Force Badge Background */
      .force-badge-bg {
        background-color: #00e676 !important;
        background-image: linear-gradient(#00e676, #00e676) !important;
      }
      /* Force Button Styles */
      .force-btn-white table td {
        background-color: #ffffff !important;
        background-image: linear-gradient(#ffffff, #ffffff) !important;
      }
      .force-btn-white a {
        color: #6200ea !important;
      }
    </mj-style>
  </mj-head>
